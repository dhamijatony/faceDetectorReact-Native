{
  "name": "react-native-vision-camera-mlkit-faces",
  "version": "1.10.0",
  "description": "Frame Processor Plugin to detect faces using MLKit Vision Face Detector for React Native Vision Camera.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/commonjs/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/commonjs/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-vision-camera-mlkit-faces-example",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "bob build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "test": "jest",
    "release": "release-it --only-version"
  },
  "keywords": [
    "vision-camera",
    "face-detector",
    "face-detection",
    "frame-processor",
    "mlkit",
    "react-native"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhamijatony/faceDetectorReact-Native.git"
  },
  "author": "tony <dhamija.tony41@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhamijatony/faceDetectorReact-Native/issues"
  },
  "homepage": "https://github.com/dhamijatony/faceDetectorReact-Native#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.74",
    "react-native-vision-camera": ">=4.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.2.0",
    "commitlint": "^19.8.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.7.0",
    "lefthook": "^2.0.3",
    "prettier": "^2.8.8",
    "react": "19.2.0",
    "react-native": "0.83.0",
    "react-native-builder-bob": "^0.40.13",
    "react-native-vision-camera": "4.7.2",
    "react-native-worklets-core": "^1.6.2",
    "release-it": "^19.0.5",
    "typescript": "^5.9.3"
  },
  "workspaces": [
    "example"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "VisionCameraMlkitFacesSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.visioncameramlkitfaces"
    }
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    }
  },
  "dependencies": {
    "@eslint/compat": "^2.0.0"
  }
}
